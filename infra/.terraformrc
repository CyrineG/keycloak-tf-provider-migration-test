# abefore running "terraform init" in terminal run :
# > export TF_CLI_CONFIG_FILE="/Users/cyrinegamoudi/keycloak-tf-provider-migration-test/infra/.terraformrc"

provider_installation {
  # First, check our local filesystem mirror for specific providers
  filesystem_mirror {
    path    = "/Users/cyrinegamoudi/keycloak-tf-provider-migration-test/infra/custom_providers"
    include = ["local/mrparkers-sorted/*"]
  }

  # For all other providers, download them directly from the public registry.
  direct {
    exclude = ["local/mrparkers-sorted/*"]
  }
}
